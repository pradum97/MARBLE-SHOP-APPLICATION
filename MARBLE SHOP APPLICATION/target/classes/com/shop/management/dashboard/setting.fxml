<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane style="-fx-background-color: #c9c6c6;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shop.management.Controller.setting">
   <top>
      <HBox fx:id="home_header" alignment="CENTER_RIGHT" minWidth="100.0" prefHeight="43.0" prefWidth="600.0" style="-fx-background-color: #3b3939;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <Button fx:id="saveBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveSetting" text="Save">
               <font>
                  <Font name="Bell MT" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </padding>
            </Button>
         </children></HBox>
   </top>
   <center>
      <VBox fx:id="containtBox" alignment="TOP_RIGHT" minWidth="700.0" prefHeight="200.0" prefWidth="1280.0" style="-fx-background-color: #dad8d8;" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <VBox>
                     <children>
                        <Label text="Theme">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font></Label>
                        <ComboBox fx:id="theme" prefWidth="150.0" promptText="select theme" />
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Font Style">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font></Label>
                        <ComboBox fx:id="fontStyle" prefWidth="150.0" promptText="Select Font Style" />
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Billing">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font>
                        </Label>
                        <ComboBox fx:id="billing" prefWidth="150.0" promptText="Select Bill" />
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>

                  <VBox>
                     <children>
                        <Label text="Igst Tax">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font>
                        </Label>
                        <ComboBox fx:id="igstTax" prefWidth="150.0" promptText="Select Igst Tax">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></ComboBox>
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>

                  <VBox>
                     <children>
                        <Label text="Sgst Tax">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font>
                        </Label>
                        <ComboBox fx:id="sgstTax" prefWidth="150.0" promptText="Select Sgst Tax">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></ComboBox>
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>

                  <VBox>
                     <children>
                        <Label text="cgst Tax">
                           <font>
                              <Font name="Berlin Sans FB Demi Bold" size="12.0" />
                           </font>
                        </Label>
                        <ComboBox fx:id="cgstTax" prefWidth="150.0" promptText="Select Cgst Tax">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></ComboBox>
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
         </children></VBox>
   </center>
</BorderPane>
